{
  "name": "Check Links",
  "description": "Checks the links to see if it is malicious or not using Google Gemini for active checking",
  "version": "1.0", 
  "manifest_version": 3, 
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "html_for_extension.html"
  }
}
